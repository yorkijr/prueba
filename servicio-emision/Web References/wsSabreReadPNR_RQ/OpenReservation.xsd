      <xsd:element minOccurs="0" name="PassengerReferences">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="PassengerRef">
              <xsd:complexType>
                <xsd:attribute name="id" />
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
