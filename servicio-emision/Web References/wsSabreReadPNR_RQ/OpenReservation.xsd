    <xsd:attribute name="id" type="xsd:string" />
  </xsd:complexType>
  <xsd:simpleType name="Numeric0to99999">
    <xsd:annotation>
      <xsd:documentation>Used for Numeric values, from 1 to 9999 inclusive.
			</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:integer">
      <xsd:minInclusive value="0" />
      <xsd:maxInclusive value="99999" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="DetailType">
    <xsd:attribute name="type" />
    <xsd:attribute name="code" />
    <xsd:attribute name="codeContext" />
  </xsd:complexType>
  <xsd:complexType name="RailwayStationInfoType" />
  <xsd:complexType name="RailAccommodationType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="PassengerRef" type="xsd:string" />
      <xsd:element name="Placement">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="Coach">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="0" name="CoachNumber" type="xsd:string" />
                  <xsd:element minOccurs="0" maxOccurs="unbounded" name="PlacementNumber">
                    <xsd:complexType>
                      <xsd:simpleContent>
                        <xsd:extension base="xsd:string">
                          <xsd:attribute name="compartmentRefId" />
                        </xsd:extension>
                      </xsd:simpleContent>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element minOccurs="0" name="CompartmentNumber" type="xsd:string" />
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="Detail" type="xsd:string" />
          </xsd:sequence>
          <xsd:attribute name="code" />
          <xsd:attribute name="type" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="Meal" type="xsd:string" />
