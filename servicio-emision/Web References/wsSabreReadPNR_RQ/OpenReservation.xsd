      <xsd:element minOccurs="0" maxOccurs="unbounded" name="StopDetails" type="LocationDetailsType" />
      <xsd:element minOccurs="0" name="EndDetails" type="LocationDetailsType" />
      <xsd:element minOccurs="0" name="RailFares" type="RailFareType" />
      <xsd:element minOccurs="0" name="Documents">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="Document" type="DocumentType" />
