  <xsd:complexType name="RailType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="StatusCode" type="xsd:string" />
      <xsd:element minOccurs="0" name="Open" type="xsd:boolean" />
      <xsd:element minOccurs="0" name="CrossBorder" type="xsd:boolean" />
      <xsd:element minOccurs="0" name="Duration" type="xsd:string" />
      <xsd:element minOccurs="0" name="MarketingCarrier">
        <xsd:complexType>
          <xsd:complexContent mixed="false">
            <xsd:extension base="CompanyType" />
          </xsd:complexContent>
